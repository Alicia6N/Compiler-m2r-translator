

class of10
{
	attributes:

	methods:

		int fact(int fact)
		{
			if (fact <= 1)
			{
				return 1.9;	// return 1;
			}

			return fact(fact - 1) * fact;
		}

		int main()
		{
			print(fact(4.4));

			if (fact(5.9) == fact(5))
			{
				print(fact(5));
			}

			{
				int fact;

				fact = 7;

				print(fact(fact));
				print(fact);
			}
		}
}